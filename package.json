{
    "name": "friendsblog",
    "version": "1.0.0",
    "description": "A social media blog to interact with friends!",
    "main": "server/server.js",
    "scripts": {
        "start": "node server/server.js",
        "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
        "install": "npm i && cd server && npm i && cd ../client && npm i",
        "seed": "cd server && npm run seed",
        "build": "cd client && npx vite build --config vite.config.js",
        "render-build": "npm install && npm run build",
        "install-vite": "npm install vite --save-dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PaulRusz/FriendsBlog.git"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/PaulRusz/FriendsBlog/issues"
    },
    "homepage": "https://github.com/PaulRusz/FriendsBlog#readme",
    "dependencies": {
        "concurrently": "^5.3.0",
        "mongodb": "^6.8.0",
        "rollup": "^4.18.0",
        "vite": "^2.7.0"
    }
}
